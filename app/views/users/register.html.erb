<h1>Registration</h1>
<form class="" action="/users/register" method = "post">
  <input type="text" name="f_name" placeholder="Full name."><br />
  <input type="text" name="street" placeholder="Street adress."><br />
  <input type="text" name="city" placeholder="City."><br />
  <input type="text" name="state" placeholder="State."><br />
  <input type="number" name="zip" placeholder="Zip Code."><br />
  <input type="text" name="country" placeholder="Country."><br />
  <input type="email" name="email" placeholder="Email."><br />
  <input type="text" name="user_id" placeholder="User ID." required><br />
  <input type="password" name="password" placeholder="Password." required><br />
  <input type="submit" value="Submit" />
</form>
